
<div class="container">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/users">Home</a></li>
      <li class="breadcrumb-item active" aria-current="page">Bulk Upload Users</li>
    </ol>
  </nav>
<h1>Bulk Upload Users</h1>
<% flash.each do |key, value| %>
  <%= content_tag :div, value, class: "flash #{key}" %>
<% end %>
<%= form_tag({action: :upload}, multipart: true) do %>
  <div class="form-group">
  <%= file_field_tag 'user_csv', class: "form-control-file" %> <br>
  <%= submit_tag("Submit", class: "btn btn-primary pull-right") %>
  </div>
<% end %>
</div>